#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#define SIZE 100

int main() {

    int n, arr[SIZE],sum = 0;
    
    scanf("%d",&n);
    
    for(int i = 0; i < n; i++){
        scanf("%d ",&arr[i]);
    }
    
    for(int i = 0; i < n; i++){
        sum += arr[i];
    }
    
    printf("%d",sum);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
