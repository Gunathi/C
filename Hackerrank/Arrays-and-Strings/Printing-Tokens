#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#define SIZE 1000

int main() {

    char s[SIZE];
    
    scanf("%[^\n]", s);
    
    for(int i = 0; i < strlen(s); i++){
        
        if(s[i] == ' '){
            printf("\n");
        }else {
            printf("%c",s[i]);
        }
    }
    
    //Write your logic to print the tokens of the sentence here.
    return 0;
}
